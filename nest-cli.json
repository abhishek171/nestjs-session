{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "identity-auth": {
      "type": "library",
      "root": "libs/identity-auth",
      "entryFile": "index",
      "sourceRoot": "libs/identity-auth/src",
      "compilerOptions": {
        "tsConfigPath": "libs/identity-auth/tsconfig.lib.json"
      }
    }
  },
  "sourceRoot": "src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  }
}